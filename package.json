{
  "name": "halfz-starter-js",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/looksofine/halfz-starter-js"
  },
  "engines": {
    "npm": ">=6",
    "node": ">=12"
  },
  "private": true,
  "author": "HALFZ Inc",
  "scripts": {
    "lint:staged": "lint-staged",
    "start": "node withBabel.js --entry=./app/server",
    "test": "jest --forceExit --coverage --detectOpenHandles --testPathIgnorePatterns admin  --testPathIgnorePatterns web",
    "build": "cross-env NODE_ENV=production && npm run build:clean && concurrently \"npm run build:server\" \"npm run build:web\" && npm run start:server:dry",
    "build:clean": "rimraf ./build/server ./build/adminWeb ./build/web",
    "build:server": "webpack --config env/webpack/webpack.server.prod.js --color",
    "build:web": "webpack --config env/webpack/webpack.web.prod.js --color --progress",
    "start:server:dry": "node build/server/prodServer.js --dry",
    "start:server": "node build/server/prodServer.js",
    "run:pm2": "pm2 start build/server/prodServer.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint  --ignore-pattern *.ts --ignore-pattern ios/** --ignore-pattern android/** --ignore-path .gitignore --ignore-pattern internals/** --ignore-pattern **/*.test.js  --ignore-pattern **/*.stories.js"
    ],
    "*.ts": [
      "eslint  --ignore-pattern ios/** --ignore-pattern android/** --ignore-path .gitignore --ignore-pattern internals/** --ignore-pattern **/*.test.js  --ignore-pattern **/*.stories.js"
    ],
    "*.tsx": [
      "eslint  --ignore-pattern ios/** --ignore-pattern android/** --ignore-path .gitignore --ignore-pattern internals/** --ignore-pattern **/*.test.js  --ignore-pattern **/*.stories.js"
    ]
  },
  "pre-commit": [
    "lint:staged"
  ],
  "dependencies": {
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "apicalypse": "^0.1.6",
    "compression": "^1.7.4",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "pm2": "^4.4.0",
    "prop-types": "latest",
    "react-device-detect": "^1.12.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
